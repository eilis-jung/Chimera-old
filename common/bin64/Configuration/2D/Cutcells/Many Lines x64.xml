  <SimulationConfig>
    <TotalTime value="10"></TotalTime>
  </SimulationConfig>
  <FlowSolverConfig>
    <FlowSolverType>CutCell</FlowSolverType>
    <SolidWallType>
      <FreeSlip></FreeSlip>
      <!--<NoSlip></NoSlip>-->
    </SolidWallType>
    <AdvectionMethod>
      <ParticleBasedAdvection>
        <PositionIntegration>
          <Method>rungeKutta2</Method>
          <!--<Interpolant>
            <Method>CubicStreamfunction</Method>
          </Interpolant>-->
        </PositionIntegration>
